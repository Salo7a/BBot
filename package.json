{
  "name": "bbot",
  "version": "0.5.0",
  "description": "Discord Multi-Purpose Bot Based on EvoBot",
  "main": "./dist/index.js",
  "private": true,
  "homepage": "https://github.com/Salo7a/BBot",
  "repository": "github:Salo7a/BBot",
  "bugs": "https://github.com/Salo7a/BBot/issues",
  "engines": {
    "node": ">=16.11.0"
  },
  "scripts": {
    "dev": "nodemon index.ts",
    "start": "node ./dist/index.js",
    "startts": "ts-node index.ts",
    "build": "tsc --build",
    "prod": "npm run build && node ./dist/index.js",
    "commit": "cz"
  },
  "dependencies": {
    "@discordjs/voice": "^0.13.0",
    "array-move": "^3.0.1",
    "discord.js": "^13",
    "dotenv": "^16.0.3",
    "ffmpeg-static": "^4.4.1",
    "i18n": "^0.15.1",
    "lyrics-finder": "^21.0.5",
    "mongoose": "*",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.6",
    "soundcloud-downloader": "^0.2.3",
    "string-progressbar": "^1.0.4",
    "ts-node": "^10.9.1",
    "youtube-sr": "~4.3.0",
    "ytdl-core": "^4.11.2",
    "ytdl-core-discord": "^1.3.1"
  },
  "devDependencies": {
    "@types/i18n": "^0.13.5",
    "@types/node": "^18.14.1",
    "cz-conventional-changelog": "^3.0.1",
    "nodemon": "^2.0.20"
  },
  "optionalDependencies": {
    "@discordjs/opus": "^0.9.0",
    "libsodium-wrappers": "^0.7.10",
    "opusscript": "^0.0.8",
    "sodium-native": "^3.4.1",
    "tweetnacl": "^1.0.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog",
      "disableScopeLowerCase": false,
      "disableSubjectLowerCase": false
